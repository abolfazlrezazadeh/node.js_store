        <% if(req.user.first_name && req.user.last_name) {%>
        <p style="font-weight: bolder;"><%= req.user.first_name+" "+req.user.last_name %></p>
        <% }else if(req.user.username){ %>
        <p style="font-weight: bolder;"><%= req.user.username%></p>
